    <s:BitmapImage source="@Embed('../../../assets/close_down.png')" includeIn="up" left="0" verticalCenter="0" fillMode="scale" />
    
    <s:BitmapImage source="@Embed('../../../assets/close_over.png')" includeIn="over" left="0" verticalCenter="0" fillMode="scale"/>

