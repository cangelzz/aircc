<?xml version="1.0" encoding="utf-8"?>
<!-- aircomponents/src/WindowSimpleChrome.mxml -->
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
    xmlns:s="library://ns.adobe.com/flex/spark" 
    xmlns:mx="library://ns.adobe.com/flex/mx"> 
     
    <fx:Style>
        @namespace "library://ns.adobe.com/flex/spark";
        WindowedApplication 
        {
            skinClass:ClassReference("spark.skins.spark.SparkChromeWindowedApplicationSkin");
            font-size:14px;
	    
        }
        
        #nameLabel {
            font-size: 20px;
        }
        
    </fx:Style>
    <fx:Script>
        import mx.collections.ArrayCollection;
        private var myList:ArrayCollection = new ArrayCollection([{"name": "aaa", "age": "1"}, {"name": "bbb", "age": "2"}]);
    </fx:Script>
    <s:VGroup height="100%" width="100%">
    <s:Label text="Hello World" height="40" />
    <s:Button label="Android" />
    <s:TextArea />
    <s:List dataProvider="{myList}" itemRenderer="Render2">
    </s:List>
    </s:VGroup>
</s:Application>